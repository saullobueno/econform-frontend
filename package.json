  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "start:qa": "env-cmd -f .env.qa react-scripts --openssl-legacy-provider start",
    "start:prod": "env-cmd -f .env.prod react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "build:qa": "env-cmd -f .env.qa react-scripts --max_old_space_size=4096 build",
    "build:prod": "env-cmd -f .env.prod react-scripts --max_old_space_size=4096 build",
    "deploy": "npm run build && aws s3 sync ./build/ s3://dev.econform.com.br",
    "deploy:qa": "npm run build:qa && aws s3 sync ./build/ s3://qa.econform.com.br",
    "deploy:prod": "npm run build:prod && aws s3 sync ./build/ s3://app.econform.com.br",
    "js-size": "node --max_old_space_size=4096",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not op_mini all"
  ],
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
